{
  "name": "redux-hyper-morph",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run watch:sass\" \"npm run start:js\"",
    "start:js": "budo . -d --serve bundle.js --live",
    "build:js": "browserify . -o bundle.js -g uglifyify",
    "build:css": "sass --update main.sass:index.css",
    "test": "browserify test/index.js | node | tap-spec",
    "watch:sass": "sass --watch main.sass:index.css",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build:js && npm run build:css && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pietgeursen/redux-hyper-morph"
  },
  "author": "pietgeursen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pietgeursen/redux-hyper-morph/issues"
  },
  "homepage": "https://github.com/pietgeursen/redux-hyper-morph",
  "dependencies": {
    "hyperscript": "^2.0.2",
    "morphdom": "^2.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "clone": "^2.0.0",
    "deep-freeze": "0.0.1",
    "gh-pages": "^0.11.0",
    "redux": "^3.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.4",
    "watchify": "^3.7.0"
  }
}
